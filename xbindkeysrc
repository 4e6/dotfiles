"xbacklight -dec 10"
  XF86MonBrightnessDown
"xbacklight -inc 10"
  XF86MonBrightnessUp

"amixer -c 0 set Master 1dB-"
  XF86AudioLowerVolume
"amixer -c 0 set Master 1dB+ unmute"
  XF86AudioRaiseVolume

"amixer -c 0 set Master toggle"
  XF86AudioMute

"i3lock -c 3f3f3f && xset dpms force off"
  XF86Display

"i3lock -c 3f3f3f"
  control+alt + l
